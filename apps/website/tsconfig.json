{
    "extends": "tsconfig/react-library.json",
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "build"
    ],
    "compilerOptions": {
        "strict": false,
        "esModuleInterop": true,
        "lib": [
            "dom",
            "esnext"
        ],
        "jsx": "react-jsx",
        "resolveJsonModule": true
    }
}